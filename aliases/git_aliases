alias gts='git status'
alias ga='git add .'
alias gp='git push'
alias gd='git diff'
alias gc='git checkout'

gcd(){
        git checkout -b "$1"
}

gm(){
	git commit -m "$1"
}

gpu(){
        git add . ; git commit -m "$1" ; git push --set-upstream origin "$2";
}

gitp(){
        git pull ; git add . ; git commit -m "$1" ; git push;
}